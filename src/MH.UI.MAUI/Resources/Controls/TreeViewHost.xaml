<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary
  xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
  xmlns:c="clr-namespace:MH.UI.MAUI.Controls;assembly=MH.UI.MAUI">

  <DataTemplate x:Key="MH.DT.TreeViewItem">
    <ViewCell>
      <Label Text="My TreeViewItem"/>
    </ViewCell>
  </DataTemplate>

  <Style TargetType="c:TreeViewHost">
    <Setter Property="ItemTemplate" Value="{StaticResource MH.DT.TreeViewItem}"/>
  </Style>
</ResourceDictionary>